@model IEnumerable<Vidly.Models.Movie>
@{
    ViewBag.Title = "index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<table class ="table table-striped table-hover ">
    <thead>
    <th>Movie</th>
    </thead>
    @foreach (var mov in Model)
    {
        <tr>
            <td>@mov.Name</td>
        </tr>
        
    }
</table>